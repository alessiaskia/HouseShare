@model IEnumerable<HouseShare.Models.BienModel>

@{
    ViewBag.Title = "MesBiens";
}

<!-- banner -->
<div class="inside-banner">
    <div class="container">
        <h2>@ViewBag.Title</h2>
    </div>
</div>
<span><a href="/Membre/Home/Index">Zone Membre</a> / @ViewBag.Title</span>
<!-- banner -->

<div class="container">
    <div class="spacer">
        <p>
            @Html.ActionLink("Rajouter un nouveau bien", "Create")
        </p>

    </div>
</div>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Titre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescCourte)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.DateCreation)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Titre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescCourte)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DateCreation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsEnabled)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisabledDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "MesBiens", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "MesBiens", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
